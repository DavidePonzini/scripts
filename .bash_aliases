alias alias-edit='nano ~/.bash_aliases'
alias cdtmp='cd `mktemp -d`'
alias chkconfig='sysv-rc-conf'
alias chmod='chmod -v'
alias chown='chown -v'
alias cls='clear'
alias cp='cp -v'
alias df='df -h --sync'
alias dmy='date +%d%m%y'
alias du='du -hd 1'
alias firefox='firefox 2> /dev/null 1>&2'
alias free='free -h | head -n 2'
alias grep='grep --color=auto'
alias ifconfigmon='exec watch -n 0.5 ifconfig'
alias la='ls -A'
alias lightdm='lighdm 2>/dev/null'
alias llk='touch /root/.llk/151130.log && logkeys --start --keymap /root/.llk/my_keymap -o /root/.llk/151130.log || echo "Failed to start llk" >&2'
alias ln='ln -v'
alias ls='ls -v --color=auto'
alias mkdir='mkdir -v'
alias mv='mv -v'
alias nano='nano -iOSxAU'
alias nautilus='nautilus 2> /dev/null'
alias open='gnome-open 2> /dev/null'
alias rm='rm -v'
alias rmdir='rmdir -v'
alias rsync='rsync -aP'
alias shred='shred -vuzn 1'
alias split='split -d'
alias ymd='date +%y%m%d'
alias youtube-dl='youtube-dl -o "%(title)s.%(ext)s"'
